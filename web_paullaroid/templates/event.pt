<tal:block metal:use-macro="load: layout.pt">        

    <nav metal:fill-slot="navigo">
       <a href="${request.route_path('home')}">Home</a>  > <a href="${request.route_path('event', event=event_name)}"> ${event_name}</a>
    </nav>
    <div id="main" metal:fill-slot="main">
        <ul>
            <li tal:repeat="image images">
                <a href="${request.route_path('image',image=image,
event=request.matchdict['event'])}">${image}</a>
            </li>
        </ul>
    </div>
</tal:block>

